      <property name="styleSheet">
       <string notr="true">/* ScrollArea için genel stil */
QScrollArea {
    border: none;
    background-color: #333; /* Koyu arka plan rengi */
}

/* ScrollArea içindeki viewport için stil */
QScrollArea &gt; QWidget &gt; QWidget {
    background-color: #333; /* Koyu arka plan rengi */
}

/* Dikey kaydırma çubuğu için stil */
QScrollArea &gt; QScrollBar:vertical {
    border: none;
    background-color: #555; /* Kaydırma çubuğu arka plan rengi */
    width: 10px; /* Kaydırma çubuğu genişliği */
    margin: 10px 0 10px 0;
}

/* Dikey kaydırma çubuğu tutamağı için stil */
QScrollArea &gt; QScrollBar:vertical &gt; QScrollBar::handle {
    background-color: #888; /* Tutamaç rengi */
    min-height: 20px; /* Tutamaç minimum yüksekliği */
    border-radius: 5px; /* Tutamaç köşe yuvarlaklığı */
}

/* Dikey kaydırma çubuğu için butonlar (yukarı/aşağı) */
QScrollArea &gt; QScrollBar:vertical &gt; QScrollBar::add-line,
QScrollArea &gt; QScrollBar:vertical &gt; QScrollBar::sub-line {
    background: none;
    border: none;
    height: 0;
}

/* Yatay kaydırma çubuğu için stil */
QScrollArea &gt; QScrollBar:horizontal {
    border: none;
    background-color: #555; /* Kaydırma çubuğu arka plan rengi */
    height: 10px; /* Kaydırma çubuğu yüksekliği */
    margin: 0 10px 0 10px;
}

/* Yatay kaydırma çubuğu tutamağı için stil */
QScrollArea &gt; QScrollBar:horizontal &gt; QScrollBar::handle {
    background-color: #888; /* Tutamaç rengi */
    min-width: 20px; /* Tutamaç minimum genişliği */
    border-radius: 5px; /* Tutamaç köşe yuvarlaklığı */
}

/* Yatay kaydırma çubuğu için butonlar (sol/sağ) */
QScrollArea &gt; QScrollBar:horizontal &gt; QScrollBar::add-line,
QScrollArea &gt; QScrollBar:horizontal &gt; QScrollBar::sub-line {
    background: none;
    border: none;
    width: 0;
}
